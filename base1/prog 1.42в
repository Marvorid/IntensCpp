#include <iostream>
using namespace std;
 
int main ()
{
    float a, b, c, d, e, f;
    cin >> a;
    b = a * a; //2-я степень
    c = b * a; //3-я
    d = c * b; //5
    e = d * d; //10
    f = e * c; //13
    cout << "a в 5-ой степени = " << c << endl;
    cout << "a в 13-ой степени = " << f;
    return 0;
}
