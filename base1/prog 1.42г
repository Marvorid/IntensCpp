#include <iostream>
using namespace std;
 
int main ()
{
    float a, b, c, d, e, f, g;
    cin >> a;
    b = a * a; //2-я степень
    c = b * b; //4-я
    d = c * a; //5
    e = d * d; //10
    f = e * c; //14
    g = f * d; //19
    cout << "a в 5-ой степени = " << d << endl;
    cout << "a в 19-ой степени = " << g;
    return 0;
}
